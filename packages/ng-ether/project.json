{
  "root": "packages/ng-ether",
  "sourceRoot": "packages/ng-ether/src",
  "prefix": "wallet-ether",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "options": {
        "project": "packages/ng-ether",
        "tsConfig": "packages/ng-ether/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/ng-ether"],
      "options": {
        "jestConfig": "packages/ng-ether/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/ng-ether/**/*.ts",
          "packages/ng-ether/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
